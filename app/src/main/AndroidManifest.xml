            android:value="@string/facebook_application_id" />
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-1385831625775129~9306990400" />
        <meta-data
            android:name="preloaded_fonts"
